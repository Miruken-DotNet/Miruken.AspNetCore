@page
<div class="container">
    <h2>Create Player</h2>
    <div class="row">&nbsp;</div>
    <div class="row">
        <div class="col-2">Full Name</div>
        <div class="col-4"><input type="text" id="playerName"/></div>
    </div>
    <div class="row">
        <div class="col-2">Date Of Birth</div>
        <div class="col-4"><input type="text" id="playerDOB"/></div>
    </div>
    <div class="row">&nbsp;</div>
    <div class="row">
        <div class="col-6">
            <input type="button" id="createPlayer" value="Create"/>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-12">
        <hr />
    </div>
</div>
<div class="row">
    <div class="col-6">
        <ul id="messagesList"></ul>
    </div>
</div>
<script src="~/js/signalr/dist/browser/signalr.js"></script>
<script src="~/js/player.js"></script>